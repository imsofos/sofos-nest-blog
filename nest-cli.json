{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/sofos-nest-blog/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/sofos-nest-blog/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/sofos-nest-blog",
  "projects": {
    "sofos-nest-blog": {
      "type": "application",
      "root": "apps/sofos-nest-blog",
      "entryFile": "main",
      "sourceRoot": "apps/sofos-nest-blog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sofos-nest-blog/tsconfig.app.json"
      }
    },
    "analytics-microservice-app": {
      "type": "application",
      "root": "apps/analytics-microservice-app",
      "entryFile": "main",
      "sourceRoot": "apps/analytics-microservice-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics-microservice-app/tsconfig.app.json"
      }
    }
  }
}